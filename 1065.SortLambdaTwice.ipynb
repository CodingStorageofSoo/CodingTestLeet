   "source": [
    "text = \"ababa\" \n",
    "words = [\"aba\",\"ab\"]"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "source": [
    "ans = []\n",
    "for word in words:\n",
    "    for i in range(len(text)):\n",
    "        if text[i:i+len(word)] == word:\n",
    "            ans.append([i,i+len(word)-1])\n",
    "ans = (sorted(ans, key = lambda x : (x[0], x[1])))"
   ],
   "outputs": [],
   "metadata": {}
  },
